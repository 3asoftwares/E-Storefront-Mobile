{
  "expo": {
    "name": "3A Storefront",
    "slug": "3asoftwares",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "astorefront",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#3B82F6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.3asoftwares.storefront",
      "jsEngine": "hermes",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan products and barcodes.",
        "NSPhotoLibraryUsageDescription": "This app uses your photo library to upload profile pictures.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.a3softwares.storefront",
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#3B82F6"
      },
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#3B82F6",
          "image": "./assets/splash-icon.png",
          "imageWidth": 200
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "85fe0bff-5263-43ba-9a68-9bb93405a534"
      }
    },
    "owner": "3asoftwares",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/85fe0bff-5263-43ba-9a68-9bb93405a534"
    }
  }
}
